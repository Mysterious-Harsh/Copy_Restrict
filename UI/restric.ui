<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="tk.Toplevel" id="mainwindow">
    <property name="geometry">1200x766</property>
    <property name="height">700</property>
    <property name="overrideredirect">True</property>
    <property name="width">540</property>
    <child>
      <object class="ttk.Labelframe" id="labelframe_2">
        <property name="height">700</property>
        <property name="labelanchor">n</property>
        <property name="relief">sunken</property>
        <property name="text" translatable="yes">labelframe_2</property>
        <property name="width">540</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Label" id="label_1">
            <property name="background">#ffffff</property>
            <property name="image">tentative software background_766x766.png</property>
            <property name="text" translatable="yes">label_1</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relx">0.13</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="filepath_E">
            <property name="font">{Hack} 12 {bold}</property>
            <property name="state">normal</property>
            <property name="takefocus">false</property>
            <property name="width">26</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property type="col" id="2" name="minsize">0</property>
              <property type="col" id="2" name="weight">0</property>
              <property type="row" id="1" name="minsize">0</property>
              <property type="row" id="1" name="uniform" />
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="filepath_B">
            <property name="command">attachment</property>
            <property name="takefocus">true</property>
            <property name="text" translatable="yes">File</property>
            <property name="width">6</property>
            <layout manager="place">
              <property name="anchor">n</property>
              <property name="height">25</property>
              <property name="relx">0.68</property>
              <property name="rely">0.12</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="encrypt_B">
            <property name="command">encrypt</property>
            <property name="default">normal</property>
            <property name="takefocus">true</property>
            <property name="text" translatable="yes">Encrypt</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="decrypt_B">
            <property name="command">decrypt</property>
            <property name="default">normal</property>
            <property name="text" translatable="yes">Decrypt</property>
            <layout manager="place">
              <property name="anchor">n</property>
              <property name="height">30</property>
              <property name="relx">0.68</property>
              <property name="rely">0.25</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="pdf_B">
            <property name="command">topdf</property>
            <property name="compound">left</property>
            <property name="default">normal</property>
            <property name="image">pdf-file-icon_30.png</property>
            <property name="state">normal</property>
            <property name="takefocus">true</property>
            <property name="text" translatable="yes">PDF</property>
            <property name="width">6</property>
            <layout manager="place">
              <property name="anchor">n</property>
              <property name="height">40</property>
              <property name="relx">0.30</property>
              <property name="rely">0.35</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="email_B">
            <property name="command">send_email</property>
            <property name="compound">left</property>
            <property name="default">normal</property>
            <property name="image">email_30.png</property>
            <property name="state">normal</property>
            <property name="takefocus">true</property>
            <property name="text" translatable="yes">E-mail</property>
            <property name="width">6</property>
            <layout manager="place">
              <property name="anchor">n</property>
              <property name="height">40</property>
              <property name="relx">0.68</property>
              <property name="rely">0.35</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
